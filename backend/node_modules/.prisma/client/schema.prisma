generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Student {
  id         String   @id @default(uuid())
  name       String
  cohort     String
  courses    String[]
  dateJoined DateTime @default(now())
  lastLogin  DateTime
  status     String   @default("Active")
}
